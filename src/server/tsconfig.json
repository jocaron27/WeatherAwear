{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "outDir": "../",
    "target": "es5",
    "module": "commonjs",
    "allowSyntheticDefaultImports": false,
    "types": [
      "node",
      "express",
      "googlemaps",
      "chai",
      "jasmine"
    ],
    "typeRoots": ["../../node_modules/@types"],
    "lib": [
      "es2015",
      "es2018"
    ]
  },
  "include": [
    "src/**/*.ts",
    "node_modules/@types"
  ],
  "exclude": [
    "node_modules"
  ]
}
