{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "outDir": "./",
    "target": "es5",
    "module": "commonjs",
    "allowSyntheticDefaultImports": false,
    "types": [
      "node",
      "express",
      "googlemaps"
    ],
    "typeRoots": ["../../node_modules/@types"],
    "lib": [
      "es2015",
      "es2018"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
